#.
#: ../lib/rb-cut-and-paste-code.c:130
msgid "%a %I:%M %p"
msgstr "%a %H:%M"

#. Translators: "friendly time" string for a day in the current year,
#. * strftime format. e.g. "Feb 12 12:34 am"
#.
#: ../lib/rb-cut-and-paste-code.c:143
msgid "%b %d %I:%M %p"
msgstr "%d de %b %H:%M"

